{
  "name": "rework-framework",
  "version": "1.0.0",
  "description": "Advanced modular FiveM framework with TypeScript, Vue, React and SQL support",
  "author": "ReWork Team",
  "license": "MIT",
  "main": "dist/client/bundle.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"client/**/*.{ts,tsx,js,jsx,vue}\"",
    "start": "npm run dev"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vue/compiler-sfc": "^3.5.25",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "vue-loader": "^17.4.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AerysYTRO/ReWork-Framework.git"
  },
  "keywords": [
    "fivem",
    "framework",
    "typescript",
    "vue",
    "react",
    "modular"
  ]
}
